<?xml version="1.0" encoding="utf-8"?>
<robot name="heri_II_NL" xmlns:xacro="http://ros.org/wiki/xacro">

	<xacro:property name="exp" value="2.71828" />

    <!-- macro to for non linear functions, that are the same for each fingers-->
  <xacro:macro name="mimic_non_linear_long_finger" params="father_joint phalange regression_level:=high">
  
  	<xacro:if value="${phalange == 'proximal'}">    <!-- joint of the first phalange -->

			<xacro:if value="${regression_level == 'high'}">
				<xacro:property name="a" value="${[0.08519, 21500000000000, -714900,  0.2555]}"/>
				<xacro:property name="b" value="${[0.9172,  32.2,           -17.93,   0.7317]}"/>
				<xacro:property name="c" value="${[0.1817,  5.513,          4.61,     1.105]}"/>
				<mimic joint="${father_joint}" nlFunPos="${a[0]}*${exp}^(-((x-${b[0]})/${c[0]})^2) + ${a[1]}*${exp}^(-((x-${b[1]})/${c[1]})^2) + ${a[2]}*${exp}^(-((x-${b[2]})/${c[2]})^2) + ${a[3]}*${exp}^(-((x-${b[3]})/${c[3]})^2)"/>
			</xacro:if>
			
    </xacro:if>
    
    
	  <xacro:if value="${phalange == 'middle'}">    <!-- joint of the 2 phalange -->
	 
			<xacro:if value="${regression_level == 'low'}">
				<xacro:property name="p" value="${[-0.2668, 0.8847 , -0.01172]}"/>
				<mimic joint="${father_joint}" nlFunPos="${p[0]}*x^2 + ${p[1]}*x + ${p[2]}"/>
			</xacro:if>
			
			<xacro:if value="${regression_level == 'medium'}">
				<xacro:property name="p" value="${[-0.4089, 0.3219, 0.679, 0.0003435]}"/>
				<mimic joint="${father_joint}" nlFunPos="${p[0]}*x^3 + ${p[1]}*x^2 + ${p[2]}*x + ${p[3]}"/>
			</xacro:if>
			
			<xacro:if value="${regression_level == 'high'}">
				<xacro:property name="p" value="${[-0.1028, 0.4311, -0.6019, 0.9364,  -0.01132]}"/>
				<mimic joint="${father_joint}" nlFunPos="${p[0]}*x^4 + ${p[1]}*x^3 + ${p[2]}*x^2 +${p[3]}*x + ${p[4]}"/>
			</xacro:if>
			
    </xacro:if>
    
    
	  <xacro:if value="${phalange == 'distal'}">    <!-- joint of the 3 phalange -->
	 
			<xacro:if value="${regression_level == 'low'}">
				<xacro:property name="p" value="${[-0.2502, 0.5836, -0.001989]}"/>
				<mimic joint="${father_joint}" nlFunPos="${p[0]}*x^2 + ${p[1]}*x + ${p[2]}"/>
			</xacro:if>
			
			<xacro:if value="${regression_level == 'medium'}">
				<xacro:property name="p" value="${[-0.06593, -0.1553, 0.5505, -0.00004385]}"/>
				<mimic joint="${father_joint}" nlFunPos="${p[0]}*x^3 + ${p[1]}*x^2 + ${p[2]}*x + ${p[3]}"/>
			</xacro:if>
			
			<xacro:if value="${regression_level == 'high'}">
				<xacro:property name="p" value="${[-0.06965, 0.4258, -0.7659, 0.3528, 0.4078, 0.007414]}"/>
				<mimic joint="${father_joint}" nlFunPos="${p[0]}*x^5 + ${p[1]}*x^4 + ${p[2]}*x^3 +${p[3]}*x^2 + ${p[4]}*x + ${p[5]}"/>
			</xacro:if>
			
    </xacro:if>
      
	</xacro:macro>
	
	
  <xacro:macro name="mimic_non_linear_short_finger" params="father_joint phalange regression_level:=high">
      
   	  <xacro:if value="${phalange == 'proximal'}">    <!-- joint of the first phalange of thumb -->
  	 
				<xacro:if value="${regression_level == 'low'}">
  				<xacro:property name="p" value="${[1.93, -1.221, 1.145, 0.001959]}"/>
					<mimic joint="${father_joint}" nlFunPos="${p[0]}*x^3 + ${p[1]}*x^2 + ${p[2]}*x + ${p[3]}"/>
				</xacro:if>
				
				<xacro:if value="${regression_level == 'high'}">
  				<xacro:property name="p" value="${[0.3378, -1.179, 1.103, 0.7363, 0.01468]}"/>
					<mimic joint="${father_joint}" nlFunPos="${p[0]}*x^4 + ${p[1]}*x^3 + ${p[2]}*x^2 + ${p[3]}*x + ${p[4]}"/>
				</xacro:if>
				
      </xacro:if>
      
   	  <xacro:if value="${phalange == 'distal'}">    <!-- joint of the 2 phalange of thumb -->
  	 
				<xacro:if value="${regression_level == 'low'}">
  				<xacro:property name="p" value="${[-1.014, 1.182, -0.009797]}"/>
					<mimic joint="${father_joint}" nlFunPos="${p[0]}*x^2 + ${p[1]}*x + ${p[2]}"/>
				</xacro:if>
				
				<xacro:if value="${regression_level == 'high'}">
  				<xacro:property name="p" value="${[-0.3366, 1.674, -2.506, 1.552, -0.02812]}"/>
					<mimic joint="${father_joint}" nlFunPos="${p[0]}*x^4 + ${p[1]}*x^3 + ${p[2]}*x^2 + ${p[3]}*x + ${p[4]}"/>
				</xacro:if>
				
      </xacro:if>
        		
	</xacro:macro>
	
